{

  "lintOptions": {
    "typeCheck": true
  },
  "extends": "tslint:all",
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "variable-name": [true, "check-format", "ban-keywords"],
    "trailing-comma": [
    ],
    "max-line-length": [false],
    "no-use-before-declare": true
  },
  "rulesDirectory": []
}
